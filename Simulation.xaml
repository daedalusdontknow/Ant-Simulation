<Window x:Class="Antsimulation.Simulation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Antsimulation"
        mc:Ignorable="d"
        Title="Simulation" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush/>
        </Grid.Background>
        <StackPanel InputScope="Default">
            <Canvas x:Name="Simulationspace" Height="434">
                <Ellipse Height="72" Canvas.Left="702" Stroke="#FFE41010" Width="72" Fill="#FFFB4008" Canvas.Top="10"/>
            </Canvas>
        </StackPanel>

    </Grid>
</Window>
